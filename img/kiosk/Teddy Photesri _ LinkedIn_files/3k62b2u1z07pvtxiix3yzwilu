o>=500?l=r.PART_UPLOAD_TRACKING_ERRORS.SERVER_ERROR:o>=400?l=r.PART_UPLOAD_TRACKING_ERRORS.CLIENT_ERROR:t.state.is.CANCELLED?l=r.PART_UPLOAD_TRACKING_ERRORS.USER_CANCELLED:0===o&&(l=r.PART_UPLOAD_TRACKING_ERRORS.NETWORK_ERROR)
var m=Date.now()-n.uploadStartTime,c=t.progress.completed||0,d=this.uploadSessionTrackingId,u=this.digitalMediaArtifactUrn,p=this.uploadMechanism
this.trackingService.fireTrackingPayload(r.UPLOAD_TRACKING_EVENTS.PART_FAILURE,{digitalMediaArtifactUrn:u,uploadSessionTrackingId:d,uploadMechanism:p,partUploadMetadata:i(e,t),responseBody:s(a),bytesTransferred:c,uploadDuration:m,errorType:l,statusCode:o})}}])
return e})()
e.default=l})

